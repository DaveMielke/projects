WSGIDaemonProcess whining user=moblab group=moblab processes=1 threads=5
WSGIScriptAlias /wmatrix /whining/run.wsgi

<Directory /whining>
    WSGIProcessGroup whining
    WSGIApplicationGroup %{GLOBAL}
    Require all granted
</Directory>
