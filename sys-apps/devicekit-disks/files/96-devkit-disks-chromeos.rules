# Do not edit this file, it will be overwritten on updates

# Do not automount Chrome OS root filesystems.
ENV{DKD_PARTITION_SCHEME}=="gpt", \
  ENV{DKD_PARTITION_TYPE}=="3CB8E202-3B7E-47DD-8A3C-7FF2A13CFCEC", \
  ENV{DKD_PRESENTATION_HIDE}="1"

# Do not automount OEM data.  OEM data doesn't have it's own gpt partition
# type, instead we key off of label and position.
ENV{DKD_PARTITION_SCHEME}=="gpt", ENV{DKD_PARTITION_NUMBER}=="8", \
  ENV{DKD_PARTITION_LABEL}=="OEM", \
  ENV{DKD_PRESENTATION_HIDE}="1"

