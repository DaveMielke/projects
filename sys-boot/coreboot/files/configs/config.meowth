CONFIG_VENDOR_GOOGLE=y
CONFIG_BOARD_GOOGLE_MEOWTH=y

CONFIG_PAYLOAD_NONE=y
CONFIG_USE_BLOBS=y

# Chrome OS
CONFIG_CHROMEOS=y

# Event Logging
CONFIG_CMOS_POST=y
CONFIG_CMOS_POST_EXTRA=y
CONFIG_CMOS_POST_OFFSET=0x70
CONFIG_ELOG_GSMI=y
CONFIG_ELOG_BOOT_COUNT=y
CONFIG_ELOG_BOOT_COUNT_CMOS_OFFSET=144

CONFIG_SPI_FLASH_SMM=y

# SPI Descriptor
CONFIG_HAVE_IFD_BIN=y
CONFIG_IFD_BIN_PATH="3rdparty/blobs/mainboard/google/meowth/descriptor.bin"

# FSP blobs
CONFIG_FSP_M_FILE="3rdparty/blobs/mainboard/google/meowth/fsp/fspm.bin"
CONFIG_FSP_S_FILE="3rdparty/blobs/mainboard/google/meowth/fsp/fsps.bin"

# Microcode
CONFIG_CPU_UCODE_BINARIES="3rdparty/blobs/mainboard/google/meowth/ucode.bin"
CONFIG_INTEL_GMA_VBT_FILE="3rdparty/blobs/mainboard/google/meowth/vbt.bin"

# Management Engine
CONFIG_HAVE_ME_BIN=y
CONFIG_ME_BIN_PATH="3rdparty/blobs/mainboard/google/meowth/me.bin"

CONFIG_ADD_FSP_BINARIES=y
CONFIG_INTEL_GMA_ADD_VBT_DATA_FILE=y
CONFIG_RUN_FSP_GOP=y
CONFIG_FSP_M_XIP=y

#Audio
CONFIG_INCLUDE_SND_MAX98373_NHLT=y
