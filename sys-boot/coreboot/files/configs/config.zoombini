CONFIG_VENDOR_GOOGLE=y
CONFIG_BOARD_GOOGLE_ZOOMBINI=y

CONFIG_PAYLOAD_NONE=y
CONFIG_USE_BLOBS=y

# Chrome OS
CONFIG_CHROMEOS=y

# Event Logging
CONFIG_CMOS_POST=y
CONFIG_CMOS_POST_EXTRA=y
CONFIG_CMOS_POST_OFFSET=0x70
CONFIG_ELOG_GSMI=y
CONFIG_ELOG_BOOT_COUNT=y
CONFIG_ELOG_BOOT_COUNT_CMOS_OFFSET=144

# SPI Descriptor
CONFIG_HAVE_IFD_BIN=n

# Management Engine
CONFIG_HAVE_ME_BIN=n
